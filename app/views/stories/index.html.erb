<h1>Story Index</h1>

<% @stories.each do |story| %>
  <a href="<%= story_path(story) %>"><img src="<%= story.photo_url %>" width="300px"/></a><br/>

  <h3>HashBack Tag:</h3>
  <p>#<%= story.hashtag %></p>
  <%= link_to "Edit", "" %>
  
  <%= form_tag "", method: "delete" do %>
    <%= submit_tag("Delete") %>
  <% end %>
  <br/><br/>

<% end %>

<br/>
<%= link_to "Add New story", new_story_path(@story) %>